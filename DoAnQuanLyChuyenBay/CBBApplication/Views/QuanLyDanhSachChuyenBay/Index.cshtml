<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        var url = "http://localhost:50947/api/";
        var listGhe = [];
        $("#MotChieu").attr('checked', 'checked');
        GetSanBayDi();
        GetSanBayDen();
        $('#NgayDi').val(today());
        function today() {
            let d = new Date();
            let currDate = d.getDate();
            let currMonth = d.getMonth() + 1;
            let currYear = d.getFullYear();
            return currYear + "-" + ((currMonth < 10) ? '0' + currMonth : currMonth) + "-" + ((currDate < 10) ? '0' + currDate : currDate);
        }
        function GetSanBayDi() {
            $.ajax({
                type: "GET",
                url: url + "SanBayDi",
                //data: $("#selectSanBayDi").serialize(),
                success: function (data) {
                    $.each(data, function (index, value) {
                        $("#selectSanBayDi").append(sanbaydiBuildTableRow(value));
                    });
                },
                error: function () { alert('error'); }
            });
        }
        function sanbaydiBuildTableRow(item) {
            var ret = '<option data-di-id= "' + item.id + '" value="' + item.tenSanBayDi + '">' + item.tenSanBayDi + '</option>';
            return ret;
        }

        function GetSanBayDen() {
            $.ajax({
                type: "GET",
                url: url + "SanBayDen",
                //data: $("#selectSanBayDi").serialize(),
                success: function (data) {
                    $.each(data, function (index, value) {
                        $("#selectSanBayDen").append(sanbaydenBuildTableRow(value));
                    });
                },
                error: function () { alert('error'); }
            });
        }
        function sanbaydenBuildTableRow(item) {
            var ret = '<option data-den-id= "' + item.id + '" value="' + item.tenSanBayDden + '">' + item.tenSanBayDden + '</option>';
            return ret;
        }

        $("#KhuHoi").click(function () {
            $('#divHide').removeAttr("hidden");

            $('#NgayDen').val(today());
            function today() {
                let d = new Date();
                let currDate = d.getDate() + 7;
                let currMonth = d.getMonth() + 1;
                let currYear = d.getFullYear();
                return currYear + "-" + ((currMonth < 10) ? '0' + currMonth : currMonth) + "-" + ((currDate < 10) ? '0' + currDate : currDate);
            }
        })
        $("#MotChieu").click(function () {
            $('#divHide').attr("hidden", true);
        });


        //danh sách chuyến bay được tìm thấy
        $("#TimChuyenBay").click(function () {
            $("#ChuyenBayTrongNgay").empty();
            var chuyenBay = new Object();
            chuyenBay.SanBayDiID = $("#selectSanBayDi option:selected").attr('data-di-id');
            chuyenBay.SanBayDenID = $("#selectSanBayDen option:selected").attr('data-den-id');

            $.ajax(
                {
                    type: "POST",
                    dataType: 'json',
                    contentType: "application/json; charset=utf-8",
                    url: url + "ChuyenBay/ChuyenBayBySanBay",
                    data: JSON.stringify(chuyenBay),
                    success: function (result) {
                        if (result.length > 0) {
                            $('#divHidenChuyeBay').removeAttr("hidden");
                            $.each(result, function (index, value) {
                                $("#ChuyenBayTrongNgay").append(chuyenbayBuildTableRow(value));
                            });
                        }
                        else {
                            $('#divHidenChuyeBay').attr("hidden", true);
                        }
                    },
                    failure: function (response) {
                        //
                    }

                });
        });

        function chuyenbayBuildTableRow(item) {
            var ret =
                '<tr>'
                + '<td>' + item.tenMayBay + '</td>'
                + '<td>' + item.gioKhoiHanh + '</td>'
                + '<td>' + item.thoiGian + ' phúc</td>'
                + '<td>' + item.gia + ' VND</td>'
                + '<td style="float:right">'
                + '<button type="button" id="ChonChuyenBayThis" style="margin-right: 10px;" data-id="' + item.id + '" class="btn btn-success btn-sm filldataechonchuyensanbay" data-toggle="modal" data-target="#chonchuyenbayModal" data-backdrop="false">Chọn Chuyến</button>'
                + '</td>'
                + '</tr>';

            return ret;
        }

        //Đặt chỗ

        $(document).on('click', '.filldataechonchuyensanbay', function () {
            $('#thongtinkhachhang').removeAttr("hidden"); 
            $('#btnDatCho').removeAttr("hidden");
            listGhe = [];
            $(".btnSoGhe").removeClass("btnColow");
            
            $('#ngaythangnamsinh').val(today());
            $("#Nam").attr('checked', 'checked');

            DataGhe();
            
        });

        //get data ghe
        function DataGhe() {
            var datcho = new Object();
            datcho.NgayDi = $('#NgayDi').val();
            if ($('#KhuHoi').is(':checked')) {
                datcho.NgayVe = $('#NgayDen').val();
            }
            datcho.ChuyenBayID = $('.filldataechonchuyensanbay').data("id");
            $.ajax(
                {
                    type: "POST",
                    dataType: 'json',
                    contentType: "application/json; charset=utf-8",
                    url: url + "DatChoHanhKhach/GiuCho",
                    data: JSON.stringify(datcho),
                    success: function (result) {
                        if (result.length > 0) {
                            for (var i = 0; i < result.length; i++) {
                                $(".btnSoGhe-" + result[i].maCho).addClass("btnColow");
                            }
                        }
                    },
                    failure: function (response) {
                        //
                    }

                });
        }

        $(document).on('click', '.btnSoGhe', function () {
            listGhe.push($(this).val());

            if ($(this).hasClass("btnColow") == true) {
                //$(this).removeClass("btnColow");

            }
            else {
                $(this).addClass("btnColow");
            }

        });

        $("#btnDatCho").click(function () {
           

            var khachHang = new Object();
            khachHang.HoTen = $('#tenkhachhang').val();
            khachHang.SoDienThoai = $('#sodienthoai').val();
            khachHang.Email = $('#emial').val();
            khachHang.NgayThangNamSinh = $('#ngaythangnamsinh').val();
            if ($('#Nam').is(':checked')) {
                khachHang.GioiTinh = true;
            }
            else {
                khachHang.GioiTinh = false;
            }

            $.ajax(
                {
                    type: "POST",
                    url: url + "DatChoHanhKhach/khachHang",
                    data: JSON.stringify(khachHang),
                    contentType: "application/json; charset=utf-8", // this
                    dataType: "json", // and this
                    //async: true,
                    success: function (result) {
                        if (result > 0) {
                            SaveDatCho(result);
                        }
                    },
                    failure: function (response) {
                        //
                    }

                });
        })
        function SaveDatCho(id) {

            var datcho = new Object();
            datcho.NgayDi = $('#NgayDi').val();
            datcho.NgayVe = $('#NgayDen').val() == "" ? null : $('#NgayDen').val();
            datcho.ChuyenBayID = $('.filldataechonchuyensanbay').data("id");
            datcho.KhachHangID = id;
            $.ajax(
                {
                    type: "POST",
                    url: url + "DatChoHanhKhach/CreateDatCho",
                    data: JSON.stringify(datcho),
                    contentType: "application/json; charset=utf-8", // this
                    dataType: "json", // and this
                    //async: true,
                    success: function (result) {
                        if (result > 0) {
                            SaveGheGiuCho(result);
                        }
                    },
                    failure: function (response) {
                        //
                    }

                });
        }
        function SaveGheGiuCho(DatChoId) {
            var giucho = new Object();
            giucho.MaCho =  listGhe.join(", ")
            $.ajax(
                {
                    type: "POST",
                    url: url + "DatChoHanhKhach/" + DatChoId +"/GheGiuCho",
                    data: JSON.stringify(giucho),
                    contentType: "application/json; charset=utf-8", // this
                    dataType: "json", // and this
                    //async: true,
                    success: function (result) {
                        if (result == true) {

                            $.toast({
                                text: "Dat ve thanh cong",
                                showHideTransition: 'slide',  // It can be plain, fade or slide
                                bgColor: 'green',              // Background color for toast
                                textColor: '#eee',            // text color
                                allowToastClose: false,       // Show the close button or not
                                hideAfter: 4000,              // `false` to make it sticky or time in miliseconds to hide after
                                stack: 3,                     // `fakse` to show one stack at a time count showing the number of toasts that can be shown at once
                                textAlign: 'right',            // Alignment of text i.e. left, right, center
                                position: 'top-right'       // bottom-left or bottom-right or bottom-center or top-left or top-right or top-center or mid-center or an object representing the left, right, top, bottom values to position the toast on page
                            })

                            $('#tenkhachhang').val("");
                            $('#sodienthoai').val("");
                            $('#emial').val("");
                            $(".btnSoGhe").removeClass("btnColow");
                            DataGhe();
                        }
                    },
                    failure: function (response) {
                        //
                    }

                });
        }
    });
</script>