                            <label for="MotChieu" class="form-check-label ">
                                <input type="radio" id="MotChieu" name="loaive" value="MotChieu" class="form-check-input">Một Chiều
                            </label>
                            <label style="margin-left:20px" for="KhuHoi" class="form-check-label ">
                                <input type="radio" id="KhuHoi" name="loaive" value="KhuHoi" class="form-check-input">Khứ Hồi
                            </label>
                        </div>
                    </div>
                </div>
